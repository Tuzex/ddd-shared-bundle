{
  "name": "tuzex/ddd-shared-bundle",
  "description": "",
  "type": "symfony-bundle",
  "license": "MIT",
  "authors": [
    {
      "name": "Edo Barnas",
      "email": "iam@edobarnas.com"
    }
  ],
  "minimum-stability": "stable",
  "config": {
    "platform": {
      "php": "8.1",
      "ext-intl": "1"
    },
    "sort-packages": true,
    "allow-plugins": {
      "composer/package-versions-deprecated": true
    }
  },
  "autoload": {
    "psr-4": {
      "Tuzex\\Bundle\\Ddd\\Shared\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Tuzex\\Bundle\\Ddd\\Shared\\Test\\": "tests/"
    }
  },
  "require": {
    "php": "^8.1",
    "symfony/framework-bundle": "5.*|6.*",
    "tuzex/ddd-shared": "dev-master"
  },
  "require-dev": {
    "phpstan/phpstan": "1.4.*",
    "phpstan/phpstan-phpunit": "1.0.*",
    "phpunit/phpunit": "9.5.*",
    "symplify/easy-coding-standard": "10.0.*"
  }
}
